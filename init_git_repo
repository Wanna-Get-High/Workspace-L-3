mkdir <repo_name>
cd <repo_name>
git init
git config --global github.user Wanna-Get-High
git config --global user.name <user_name>
git config --global user.email <email_adress>
git remote add origin https://Wanna-Get-High@github.com/Wanna-Get-High/Workspace-L-3.git
git config branch.master.remote origin
git config branch.master.merge refs/heads/master
git pull

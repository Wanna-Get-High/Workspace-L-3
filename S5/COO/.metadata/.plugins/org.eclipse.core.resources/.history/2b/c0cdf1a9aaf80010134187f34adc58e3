package oie;

public class TeleportCell extends Cell
{
	public TeleportCell(int ind,int jmpInd)
	{
		super(ind);
	}
	
	public boolean canBeLeft() 
	{
		return true;
	}

	public int consequence(int diceThrow) 
	{
		return diceThrow;
	}

	public void setPlayer(Player player) 
	{		
		this.player = player;
	}
}

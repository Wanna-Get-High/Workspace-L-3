package mail;

public class Simulation 
{
	public Adress createAdress(Town t)
	{
		return new Adress((int)Math.random(), "troud bal", t);
	}
	
	public Inhabitant[] createInhabitant(int lenght, Town t)
	{
		Inhabitant[] in = new Inhabitant[lenght];
		for (int i = 0;i< length;i++)
			in[i] = new Inhabitant((float)Math.random(),createAdress(t));
		
		return in;
	}
	

package action;

public class Wait extends Action 
{
	
	protected int timeToWait;
	
	public Wait(String msg, int ttw)
	{
		super(msg);
		this.timeToWait = ttw;
	}

	public Wait(int ttw) 
	{
		this("", ttw);
	}

	@Override
	public void inToDo() 
	{
		// TODO Auto-generated method stub

	}

	@Override
	protected boolean stopCondition() {
		// TODO Auto-generated method stub
		return false;
	}

}

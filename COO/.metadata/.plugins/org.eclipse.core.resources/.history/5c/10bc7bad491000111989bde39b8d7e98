package questionnaires;

/**
 * The Class Symbolique.
 */
public class Symbolique extends Answer<String>
{	
	
	/**
	 * Instantiates a new symbolique.
	 *
	 * @param s the string 
	 */
	public Symbolique(String s)
	{
		super(s);
	}
	
	/**
	 * Builds the symbolique answer.
	 *
	 * @param answerText the answer text
	 * @return the answer
	 * @throws IllegalArgumentException the illegal argument exception
	 */
	public static Answer<?> build(String answerText) throws IllegalArgumentException
	{
		return new Symbolique(answerText);
	}

    @Override
    public String getType() 
    {        
        return "symbolique";
    }

    /**
     * return true if the text is a String.
     * @param text the text to check.
     */
    public boolean acceptablePattern(String text) 
    {
        return true;
    }
}
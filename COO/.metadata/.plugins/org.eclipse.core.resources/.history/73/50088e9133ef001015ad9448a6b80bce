package afficheurLumineux;


public class Afficheur
{
	private String message; 
	private FileFIFO<Character> f;
	/**
	 */
	public void setMessage(String msg)
	{
		this.message = msg;
	}
			
	/**
	 */
	public void decale()
	{
		f.add(this.message.charAt(f.head));
	}

	/**
	 */
	public String toString()
	{
		return "";	
	}
	
	
}
